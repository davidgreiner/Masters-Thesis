@article{Chakraborty2008,
	abstract = {This paper presents a technique for view invariant human detection and$\backslash$nextending this idea to recognize basic human actions like walking,$\backslash$njogging, hand waving and boxing etc. To achieve this goal we detect the$\backslash$nhuman in its body parts and then learn the changes of those body parts$\backslash$nfor action recognition. Human-body part detection in different views is$\backslash$nan extremely challenging problem due to drastic change of 3D-pose of$\backslash$nhuman body, self occlusions etc while performing actions. In order to$\backslash$ncope with these problems we have designed three example-based detectors$\backslash$nthat are trained to find separately three components of the human body,$\backslash$nnamely the head, legs and arms. We incorporate 10 sub-classifiers for$\backslash$nthe head, arms and the leg detection. Each sub-classifier detects body$\backslash$nparts under a specific range of viewpoints. Then, view-invariance is$\backslash$nfulfilled by combining the results of these sub classifiers.$\backslash$nSubsequently, we extend this approach to recognize actions based on$\backslash$ncomponent-wise Hidden Markov Models (HMM). This is achieved by designing$\backslash$na HMM for each action, which is trained based on the detected body$\backslash$nparts. Consequently, we are able to distinguish between similar actions$\backslash$nby only considering the body parts which has major contributions to$\backslash$nthose actions e.g. legs for walking, running etc; hands for boxing,$\backslash$nwaving etc.},
	author = {Chakraborty, Bhaskar and Rudovic, Ognjen and Gonz{\`{a}}lez, Jordi},
	doi = {10.1109/AFGR.2008.4813302},
	isbn = {9781424421541},
	journal = {IEEE International Conference on Automatic Face and Gesture Recognition},
	title = {{View-invariant human-body detection with extension to human action recognition using component-wise HMM of body parts}},
	year = {2008},
	pages={1-6}
}

