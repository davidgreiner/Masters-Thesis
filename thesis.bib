	issn = {1527-5418},
	journal = {MobiSys ... : the ... International Conference on Mobile Systems, Applications and Services. International Conference on Mobile Systems, Applications, and Services},
	keywords = {inertial measurement unit,mobile computing,smoking detection,wearables},
	pages = {149--161},
	pmid = {26688835},
	title = {{RisQ: Recognizing Smoking Gestures with Inertial Sensors on a Wristband.}},
	url = {http://dl.acm.org/citation.cfm?id=2594368.2594379},
	volume = {2014},
